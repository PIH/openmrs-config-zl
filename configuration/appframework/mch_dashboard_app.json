        "extensionParams": {
          "provider": "coreapps",
          "fragment": "dashboardwidgets/dashboardWidget"
        }
      }
    ],
    "config": {
      "widget": "obsgraph",
      "icon": "fas fa-fw fa-chart-bar",
      "label": "pih.app.hivWeightGraph.title",
      "conceptId": "${concept.weightKG.uuid}",
      "duringCurrentEnrollmentInProgram": "${program.mch.uuid}",
